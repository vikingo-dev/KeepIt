{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@models/*": [
        "./src/types/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@ui/*": [
        "./src/components/ui/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
    }
  }
}